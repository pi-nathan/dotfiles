[core]
	editor = code --wait
[alias]
	yeet = push
	yoink = pull
	br = checkout -b
	pushup = !git push -u origin $(git symbolic-ref --short HEAD)
	rename-branch = branch -m
	undo-file = checkout origin/master
	amend = commit --amend --no-edit
	rollback = "!git reset @~${1:-1} #"
